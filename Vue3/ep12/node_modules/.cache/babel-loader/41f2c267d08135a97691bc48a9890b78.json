{"ast":null,"code":"import myPeople from \"./components/myPeople.vue\";\nimport { ref } from \"vue\";\nexport default {\n  name: \"App\",\n\n  setup() {\n    let Tjobs = ref(\"Teacher\");\n    let Sjobs = ref(\"Student\");\n    let Tnumber = ref(1);\n    let Snumber = ref(10);\n    let timer = null;\n    let btnText = ref(\"ON\");\n\n    function set() {\n      if (timer) {\n        clearInterval(timer);\n        btnText.value = \"ON\";\n      } else {\n        timer = setInterval(() => {\n          Tnumber.value++;\n          Snumber.value += 10;\n        }, 1000);\n        btnText = ref(\"OFF\");\n      }\n    }\n\n    return {\n      Tjobs,\n      Sjobs,\n      Tnumber,\n      Snumber,\n      timer,\n      btnText,\n      set\n    };\n  },\n\n  components: {\n    myPeople\n  }\n};","map":{"version":3,"mappings":"AAgBA,OAAOA,QAAP,MAAqB,2BAArB;AACA,SAAQC,GAAR,QAAkB,KAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;;EAEbC,KAAK,GAAG;IACN,IAAIC,KAAI,GAAIH,GAAG,CAAC,SAAD,CAAf;IACA,IAAII,KAAI,GAAIJ,GAAG,CAAC,SAAD,CAAf;IACA,IAAIK,OAAM,GAAIL,GAAG,CAAC,CAAD,CAAjB;IACA,IAAIM,OAAM,GAAIN,GAAG,CAAC,EAAD,CAAjB;IACA,IAAIO,KAAI,GAAI,IAAZ;IACA,IAAIC,OAAM,GAAIR,GAAG,CAAC,IAAD,CAAjB;;IAEA,SAASS,GAAT,GAAc;MACZ,IAAGF,KAAH,EAAS;QACPG,aAAa,CAACH,KAAD,CAAb;QACAC,OAAO,CAACG,KAAR,GAAgB,IAAhB;MACD,CAHD,MAGK;QACHJ,KAAI,GAAIK,WAAW,CAAC,MAAI;UACtBP,OAAO,CAACM,KAAR;UACAL,OAAO,CAACK,KAAR,IAAe,EAAf;QACD,CAHkB,EAGjB,IAHiB,CAAnB;QAIAH,OAAM,GAAIR,GAAG,CAAC,KAAD,CAAb;MACF;IACF;;IAEA,OAAO;MACLG,KADK;MAELC,KAFK;MAGLC,OAHK;MAILC,OAJK;MAKLC,KALK;MAMLC,OANK;MAOLC;IAPK,CAAP;EASD,CAhCY;;EAiCbI,UAAU,EAAE;IACVd;EADU;AAjCC,CAAf","names":["myPeople","ref","name","setup","Tjobs","Sjobs","Tnumber","Snumber","timer","btnText","set","clearInterval","value","setInterval","components"],"sourceRoot":"","sources":["/Users/huanglangxuan/Desktop/Vue考试期末参考/Vue3/实验12 单文件组件/src/App.vue"],"sourcesContent":["<template>\n  <div>\n    <myPeople :job=\"Tjobs\" :number=\"Tnumber\">\n      <button @click=\"set\">{{ btnText }}</button>\n    </myPeople>\n    <hr />\n    <myPeople :job=\"Sjobs\" :number=\"Snumber\">\n      <img\n        src=\"https://www.gkd.edu.cn/upload/images/2022/3/189e568bed4b5bdf.jpg\"\n        alt=\"广科大学生图片\"\n      />\n    </myPeople>\n  </div>\n</template>\n\n<script>\nimport myPeople from \"./components/myPeople.vue\";\nimport {ref} from \"vue\";\n\nexport default {\n  name: \"App\",\n  setup() {\n    let Tjobs = ref(\"Teacher\");\n    let Sjobs = ref(\"Student\");\n    let Tnumber = ref(1);\n    let Snumber = ref(10);\n    let timer = null;\n    let btnText = ref(\"ON\");\n\n    function set(){\n      if(timer){\n        clearInterval(timer);\n        btnText.value = \"ON\";\n      }else{\n        timer = setInterval(()=>{\n          Tnumber.value++;\n          Snumber.value+=10;\n        },1000);\n        btnText = ref(\"OFF\");\n      }\n    }\n\n    return {\n      Tjobs,\n      Sjobs,\n      Tnumber,\n      Snumber,\n      timer,\n      btnText,\n      set\n    }\n  },\n  components: {\n    myPeople,\n  },\n};\n</script>\n\n"]},"metadata":{},"sourceType":"module"}